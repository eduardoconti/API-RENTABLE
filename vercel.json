{
  "version": 2,
  "env": {
    "DATABASE_URL": "@DATABASE_URL",
    "SECRET": "@SECRET",
    "CLOUDINARY_NAME": "@CLOUDINARY_NAME",
    "CLOUDINARY_API_KEY": "@CLOUDINARY_API_KEY",
    "CLOUDINARY_API_SECRET": "@CLOUDINARY_API_SECRET"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
